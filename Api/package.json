{
  "name": "warung-ibuk-iyos-backend",
  "version": "1.0.0",
  "description": "Backend API for Warung Ibuk Iyos",
  "main": "dist/server.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "start": "node dist/server.js",
    "predev": "npx prisma generate --schema prisma/schema.prisma",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "db:generate": "npx prisma generate --schema prisma/schema.prisma",
    "db:push": "npx prisma db push --schema prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema prisma/schema.prisma",
    "db:reset": "npx prisma migrate reset --schema prisma/schema.prisma --force",
    "init": "node ./scripts/check-env.js && npm run db:generate && npm run db:push && node ./scripts/create-admin-direct.js",
    "postinstall": "npx prisma generate --schema prisma/schema.prisma",
    "create-admin": "node ./scripts/create-admin-direct.js",
    "seed-products": "node ./scripts/seed-products.js",
    "get-token": "node ./scripts/get-token.js",
    "gen-token": "node ./scripts/generate-token.js",
    "demo": "node ./scripts/demo.js",
    "postbuild": "node ./scripts/copy-openapi.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.4-lts.1",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "quickchart-js": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "@types/pdfkit": "^0.12.9",
    "prisma": "^5.22.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
    ,"@types/js-yaml": "^4.0.3"
  }
}
